{
    "name": "joseki/migration",
    "type": "library",
    "description": "Joseki/Migration is console database migration tool for your PHP apps.",
    "keywords": ["joseki", "migrations", "database", "db", "database migrations"],
    "license": "MIT",
    "authors": [{
        "name": "Miroslav Paulik",
        "email": "miras.paulik@seznam.cz",
        "homepage": "http://mirapaulik.cz",
        "role": "Lead Developer"
    }],
    "require": {
        "php": ">=5.3.2",
        "dibi/dibi": "~3.0",
        "doctrine/dbal": "2.4.2",
        "joseki/console": "~0.1",
        "joseki/utils": "~1.0",
        "nette/di": "~2.2",
        "nette/finder": "~2.2",
        "nette/php-generator": ">=2.3.4",
        "nette/reflection": "~2.2",
        "nette/robot-loader": "~2.2",
        "symfony/console": "~2.7",
        "symfony/config": "~2.7",
        "symfony/yaml": "~2.7",
        "tharos/leanmapper": "dev-develop"
    },
    "require-dev": {
        "joseki/leanmapper-extension": "dev-master",
        "joseki/leanmapper-query": "dev-master",
        "nette/bootstrap": "~2.2",
        "nette/tester": "~1.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "autoload-dev": {
        "classmap": ["tests"]
    }
